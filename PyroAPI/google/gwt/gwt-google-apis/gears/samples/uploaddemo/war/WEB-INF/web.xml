<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>UploadDemo.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>uploadServlet</servlet-name>
    <servlet-class>com.google.gwt.gears.sample.uploaddemo.server.UploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>uploadServlet</servlet-name>
    <url-pattern>/uploaddemo/upload</url-pattern>
  </servlet-mapping>
</web-app>
