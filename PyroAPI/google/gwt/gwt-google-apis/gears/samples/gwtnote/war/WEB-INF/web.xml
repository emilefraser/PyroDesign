<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>GWTNote.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>noteServiceImpl</servlet-name>
    <servlet-class>com.google.gwt.gears.sample.gwtnote.server.NoteServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>noteServiceImpl</servlet-name>
    <url-pattern>/gwtnote/rpc</url-pattern>
  </servlet-mapping>
</web-app>
